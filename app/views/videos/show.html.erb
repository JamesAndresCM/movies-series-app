<%= render @videos %>
<div class="mt-2 flickr_pagination">
  <% unless @videos.empty? or @videos.nil? %>
    <%= will_paginate @videos, class:'video', id: 'video', :page_links => false %>
  <% end %>
</div>
<%= link_to "Back To Chapters", series_path(@serie), class:'btn btn-primary' %>
<div class="mt-4 container col-sm-10" style="background-color: white" id="disqus_thread"></div>
<div id="disqus_thread"></div>
<script>

var disqus_config = function () {
      this.page.url = '<%= request.original_url %>';  // Replace PAGE_URL with your page's canonical URL variable
      this.page.identifier = '<%= @videos %>'; // Replace PAGE_IDENTIFIER with your page's unique identifier variable
    };

(function() { // DON'T EDIT BELOW THIS LINE
  var d = document, s = d.createElement('script');
  s.src = 'YOUR_URL';
  s.setAttribute('data-timestamp', +new Date());
  (d.head || d.body).appendChild(s);
})();
</script>
<noscript>Please enable JavaScript to view the <a href="https://disqus.com/?ref_noscript">comments powered by Disqus.</a></noscript>