<% unless @series.blank? %>
  <div style="display: inline-block;" class="mt-5 container">
    <div id="series" class="card-columns col-md-12" style="margin: 0 auto">
      <% cache @series do %>
        <%= render @series, cached: true %>
      <% end %>
    </div>
  </div>
<% else %>
  <%= render "partials/not_found" %>
<% end %>
